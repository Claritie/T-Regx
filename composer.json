{
  "name": "rawr/t-regx",
  "version": "1.0",
  "type": "library",
  "description": "The most advanced PHP regexp library. Clean, descriptive wrapper functions enhancing PCRE extension methods.",
  "license": "MIT",
  "keywords": [
    "php",
    "t-regx",
    "t-rex",
    "regex",
    "regexp",
    "regular",
    "expression",
    "tdd",
    "oop",
    "clean",
    "design"
  ],
  "autoload": {
    "files": [
      "helper/helper.php"
    ],
    "psr-4": {
      "TRegx\\CleanRegex\\": "src/TRegx/CleanRegex",
      "TRegx\\SafeRegex\\": "src/TRegx/SafeRegex"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ]
  },
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "satooshi/php-coveralls": "^2.0"
  },
  "archive": {
    "exclude": [
      "/test/**",
      "/test"
    ]
  }
}
